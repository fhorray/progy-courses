{
  "id": "rust-flow",
  "name": "Rust Flow",
  "runner": {
    "command": "cargo",
    "args": ["run", "-q", "-p", "runner", "--", "test", "content/{{id}}"],
    "cwd": "."
  },
  "content": {
    "root": ".",
    "exercises": "content"
  },
  "setup": {
    "checks": [
      {
        "name": "Rust Compiler",
        "type": "command",
        "command": "rustc --version"
      },
      {
        "name": "Cargo Package Manager",
        "type": "command",
        "command": "cargo --version"
      }
    ],
    "guide": "SETUP.md"
  }
}
